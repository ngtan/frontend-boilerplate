{
  "name": "frontend-boilerplate",
  "version": "1.1.0",
  "description": "Make the development process as fast as possible",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngtan/frontend-boilerplate.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.10.0"
  },
  "author": "Tan Nguyen",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config ./internals/webpack/webpack.config.dev.js",
    "build": "webpack --config ./internals/webpack/webpack.config.prod.js --color -p --progress --hide-modules --display-optimization-bailout",
    "lint:js": "eslint --ignore-path .gitignore .",
    "lint:css": "stylelint \"./app/**/*.scss\"",
    "test": "npm run lint:js && npm run lint:css"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "jquery": "3.3.1",
    "modernizr": "3.6.0",
    "sanitize.css": "8.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.1.0",
    "autoprefixer": "9.3.1",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "clean-webpack-plugin": "0.1.19",
    "css-loader": "1.0.0",
    "eslint": "5.7.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "file-loader": "2.0.0",
    "glob": "7.1.3",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "4.4.0",
    "json-loader": "0.5.7",
    "mini-css-extract-plugin": "0.4.4",
    "modernizr-loader": "1.0.1",
    "node-sass": "4.9.4",
    "postcss-loader": "3.0.0",
    "pug": "2.0.3",
    "pug-loader": "2.4.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "stylelint": "9.6.0",
    "stylelint-config-recommended-scss": "3.2.0",
    "stylelint-scss": "3.3.2",
    "webpack": "4.23.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-pwa-manifest": "3.7.1"
  }
}
